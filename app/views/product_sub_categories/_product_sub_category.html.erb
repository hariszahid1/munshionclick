<tr>
  <td><%= check_box_tag 'object_ids[]', product_sub_category.id %></td>
  <td><%= product_sub_category.id %></td>
  <td><%= product_sub_category.product_category.title %></td>
  <td class= 'urdu-font'><%= product_sub_category.title %></td>
  <td><%= product_sub_category.code %></td>
  <td><%= product_sub_category.comment %></td>
  <td>
      <div class="icon-container-1">
                <div class="my-icon-1 my-view-1">
                  <div class="my-tooltip-1">
          <div class="icon-container">
          <div class="my-icon my-view">
          <div class="my-tooltip">
            Edit
          </div>
          <span class="icon-div mt-3">
                <% if can?(:update, :all) %>
              <%= link_to edit_product_sub_category_path(product_sub_category), remote: true do %>
                <i class="fa fa-edit mt-2" aria-hidden="true"></i>
              <%end %>
            <% end %>
          </span>
        </div>

          <div class="my-icon my-view">
              <div class="my-tooltip">
                Delete
              </div>
                <span class="icon-div mt-3">
                  <% if can?(:delete, :all) %>
                    <%= link_to product_sub_category, method: :delete, data: {title: 'Confirmation' ,confirm: 'Are you sure?', commit: 'Sure!'}, :remote => true, :class => 'delete' do %>
                    <i class="fa fa-trash mt-2" aria-hidden="true"></i>
                  <%end %>
                <% end %>
              </span>
          </div>

      <div class="my-icon my-view">
                <div class="my-tooltip">
                  Show
                </div>
                <span class="icon-div mt-3">
                   <% if can?(:view, :all) %>
                    <%= link_to product_sub_category_path(product_sub_category), remote: true do %>
                    <i class="fa fa-eye mt-2" aria-hidden="true"></i>
                <%end %>
            <%end%>
          </span>
      </div>

      <div class="my-icon my-view">
            <div class="my-tooltip">
              PDF
            </div>
            <span class="icon-div mt-3">
                <%= link_to product_sub_categories_path('q[id_eq]':product_sub_category.id,'pdf':true), title: 'Download PDF', target: '_blank' do %>
			<i class="fa fa-file-pdf text-danger mt-2 " aria-hidden="true"></i>
		<%end %>
            </span>
      </div>

       <div class="my-icon my-view">
            <div class="my-tooltip">
              CSV
            </div>
            <span class="icon-div mt-3">
                 <%= link_to product_sub_categories_path('q[id_eq]':product_sub_category.id,'csv':true), title: 'Download CSV' do%>
			<i class="fa fa-file-excel mt-2 text-success" aria-hidden="true"></i>
		<%end %>
            </span>
          </div>
    </div>
          </div>
          <span class="icon-div-1">
                <i class="fa fa-ellipsis-v mt-2" aria-hidden="true"></i>
          </span>
      </div>
    </div>

  </td>
</tr>
