<div class="nested-fields">
  <%= f.hidden_field :purchase_sale_detail_id %>
  <%= f.hidden_field :id,value: nil %>
  <%= f.hidden_field :transaction_type%>
  <%= f.hidden_field :product_id%>
  <%= f.hidden_field :item_id%>
  <%= f.hidden_field :purchase_sale_type, value: "Return_type"%>
  <div class="col-lg-12">
    <div class="col-lg-6 float-left">
      <%= f.number_field :quantity,step: :any,class:'quantity-price', hide_label: true,placeholder: :quantity,onchange:"set_return_sale_total(this)",onkeyup:"set_return_sale_total(this)",min: 0,max: f.object.quantity%>
      <%= f.hidden_field :sale_price,step: :any,class:'cost-price',hide_label: true, placeholder: :sale_price %>
      <%= f.hidden_field :total_sale_price,step: :any,class:'total-cost-price', hide_label: true,placeholder: :total_sale%>
      <%=f&.object&.product&.title%>
    </div>
    <div class="col-lg-6 float-left">
      <%= f.hidden_field :comment,step: :any,class:'stock', hide_label: true,placeholder: :Stock,readonly: true%>
      <%= f.hidden_field :discount_price,step: :any,class:'discount-price', hide_label: true,placeholder: :discount%>
      <%=f.hidden_field :extra_quantity,value:0,step: :any,class:'extra-quantity', hide_label: true,placeholder: :extra%>
    </div>
  </div>
</div>
