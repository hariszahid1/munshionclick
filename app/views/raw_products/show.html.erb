

  <div class="card shadow mb-4">
      <!-- Card Header - Accordion -->
      <a href="#collapseCardExample" class="d-block card-header py-3" data-toggle="collapse" role="button" aria-expanded="true" aria-controls="collapseCardExample">
        <h6 class="m-0 font-weight-bold text-primary"><h1>Raw product Details</h1></h6>
      </a>
      <!-- Card Content - Collapse -->
      <div class="collapse show" id="collapseCardExample" style="">
        <div class="card-body">

  <div class="row">
          <div class="col-lg-12 col-sm-12">
            <div class="col-lg-3 form-group float-left card mb-4 py-3 border-left-primary shadow h-100 py-2">
              <h3>Id:<h5> <%= @raw_product.id %></h5></h3>
            </div>
            <div class="col-lg-3 form-group float-left card mb-4 py-3 border-left-primary shadow h-100 py-2">
              <h3>Title:<h5><%= @raw_product.title %></h5></h3>
            </div>
            <div class="col-lg-3 form-group float-left card mb-4 py-3 border-left-primary shadow h-100 py-2">
              <h3>Code:<h5><%= @raw_product.code %></h5> </h3>
            </div>
            <div class="col-lg-3 form-group float-left card mb-4 py-3 border-left-primary shadow h-100 py-2">
              <h3>Stock:<h5><%= @raw_product.stock %></h5></h3>
            </div>
          </div>
  </div>

  <div class="row">
          <div class="col-lg-12 col-sm-12">
            <div class="col-lg-3 form-group float-left card mb-4 py-3 border-left-primary shadow h-100 py-2">
              <h3>Acquire Type: <h5><%= @praw_product.acquire_type rescue nil %></h5></h3>
            </div>
            <div class="col-lg-3 form-group float-left card mb-4 py-3 border-left-primary shadow h-100 py-2">
              <h3>Cost:<h5><%= @raw_product.cost %></h5></h3>
            </div>
            <div class="col-lg-3 form-group float-left card mb-4 py-3 border-left-primary shadow h-100 py-2">
              <h3>Sale:<h5><%= @raw_product.sale %></h5> </h3>
            </div>
            <div class="col-lg-3 form-group float-left card mb-4 py-3 border-left-primary shadow h-100 py-2">
              <h3>MIN:<h5><%= @raw_product.minimum %></h5></h3>
            </div>
          </div>
  </div>

  <div class="row">
          <div class="col-lg-12 col-sm-12">
            <div class="col-lg-3 form-group float-left card mb-4 py-3 border-left-primary shadow h-100 py-2">
              <h3>Optimal:<h5><%= @raw_product.optimal %></h5></h3>
            </div>
            <div class="col-lg-3 form-group float-left card mb-4 py-3 border-left-primary shadow h-100 py-2">
              <h3>MAX:<h5><%= @raw_product.maximum %></h5></h3>
            </div>


  </div>
  </div>


          <div class="row">
            <div class="col-lg-1 float-left">
              <%= link_to products_path do %>
              <i class="fa fa-arrow-left " aria-hidden="true"></i>
              <% end %>
            </div>
          </div>

        </div>
      </div>
    </div>
