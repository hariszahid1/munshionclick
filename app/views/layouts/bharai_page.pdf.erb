<!DOCTYPE html>
<html>
<head>
    <title>POS</title>
    <%= wicked_pdf_stylesheet_link_tag "pdf" -%>
</head>
<%@total_bil=0%>
<%@total_am=0%>
<%@total_dis=0%>
<%@total_pro=0%>
<%@total_discont=0%>


<body class="bond_pos_pdf">
  <% @time = Time.zone.now %>
  <% if @pos_setting.present? && @pos_setting.persisted? && @pos_setting.logo_images.present? %>
    <div class="col-lg-12">
      <%= image_tag @pos_setting.logo_images.first.service_url, width: 200, class:"pdf_img" %>
    </div>
  <%else%>
  <% end %>
  <h1 style="text-align: center; margin: 0;">Bharai Entry page</h1>
  <div>
    <table>
      <tbody>
        <tr>
          <td class="broder-with"><h5>Start Date:</h5></td>
          <td class="broder-with"><h5>End Date:</h5></td>
          <td class="broder-with"><h5>Comment:</h5></td>

        </tr>
      </tbody>
    </table>
    <table>
      <thead>
        <th>ID</th>
        <th>Block Type</th>
        <th>Brick Quantity</th>
        <th>Tile Quantity</th>
        <th>Date</th>
        <th></th>
      </thead>
      <tbody>
        <% for i in 1..150 do %>
        <tr>
          <td class="broder-with" style="text-align :center"><b><%=i%></b></td>
          <td class="broder-with" style="text-align :center"><h5> Small|Medium|large|Mix</h5></td>
          <td class="broder-with"></td>
          <td class="broder-with"></td>
          <td class="broder-with"></td>
        </tr>
        <%end%>
      </tbody>

    </table>
  </div>
  <p style="text-align: center;margin: 0;"><span><%=@pos_setting.address if @pos_setting.present?%></span></p>
  <p style="text-align: center;margin: 0;"><span><%=@pos_setting.phone if @pos_setting.present?%></span></p>
</body>
</html>
