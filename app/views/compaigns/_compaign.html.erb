<tr>
  <td><%= compaign.id %></td>
  <td class='urdu-font'><%= compaign.title %></td>
  <td><%= compaign.comment %></td>
  <td>
    <%if compaign.created_at.strftime("%d%b%y")!=compaign.updated_at.strftime("%d%b%y")%>
      <%= compaign.created_at.present? ? compaign.created_at.strftime("%d%b%y at %H:%M%p") : '' %> | <%= compaign.updated_at.present? ? compaign.updated_at.strftime("%d%b%y at %H:%M%p") : '' %>
    <%else%>
      <%= compaign.created_at.present? ? compaign.created_at.strftime("%A,  %d-%b-%y at %H:%M%p") : '' %>
    <%end%>
  </td>
  <td>
  <div class="icon-container-1">
      <div class="my-icon-1 my-view-1">
        <div class="my-tooltip-1">
          <div class="icon-container">
            <div class="my-icon my-view">
              <div class="my-tooltip">
                Edit
              </div>
              <%if (check_can_update(@module_permission))%>
                <%= link_to edit_compaign_path(compaign), class:"icon-div" do %>
                  <i class="fa fa-edit mt-2" aria-hidden="true"></i>
                <%end %>
              <% end %>
            </div>
            <div class="my-icon my-view">
              <div class="my-tooltip">
                Delete
              </div>
              <%if (check_can_delete(@module_permission))%>
                <%= link_to compaign, method: :delete, data: {title: 'Confirmation' ,confirm: 'Are you sure?', commit: 'Sure!'}, :remote => true, :class => 'delete icon-div' do %>
                  <i class="fa fa-trash mt-2" aria-hidden="true"></i>
                <%end %>
              <% end %> 
            </div>
            <div class="my-icon my-view">
              <div class="my-tooltip">
                Show
              </div>
              <%if (check_can_read(@module_permission))%>
                <%= link_to compaign_path(compaign), class:"icon-div" do %>
                  <i class="fa fa-eye mt-2" aria-hidden="true"></i>
                <%end %>
              <%end%>    
            </div>
            
            
          </div>
        </div>
        <span class="icon-div-1">
              <i class="fa fa-ellipsis-v mt-2" aria-hidden="true"></i>
        </span>
      </div>
    </div>
  </td>
</tr>
