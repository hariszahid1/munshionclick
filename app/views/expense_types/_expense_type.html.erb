<tr>
  <td><%= check_box_tag 'object_ids[]', expense_type.id %></td>
  <td><%= expense_type.id %></td>
  <td class='urdu-font'><%= expense_type.title %></td>
  <td><%= expense_type.comment %></td>
  <td>
    <% if can?(:update, :all) %>
      <%= link_to edit_expense_type_path(expense_type) do %>
        <i class="fa fa-edit " aria-hidden="true"></i> |
      <%end %>
    <% end %>
    <% if can?(:delete, :all) %>
      <%= link_to expense_type, method: :delete, data: {title: 'Confirmation' ,confirm: 'Are you sure?', commit: 'Sure!'}, :remote => true, :class => 'delete' do %>
        <i class="fa fa-trash " aria-hidden="true"></i> |
      <%end %>
    <% end %>
    <% if can?(:view, :all) %>
      <%= link_to expense_type do %>
        <i class="fa fa-eye " aria-hidden="true"></i>
      <%end %>
    <%end%>
  </td>
</tr>
