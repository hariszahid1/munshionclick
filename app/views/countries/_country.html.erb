<tr>
  <td><%= check_box_tag 'object_ids[]', country.id %></td>
  <td><%= country.id %></td>
  <td class= 'urdu-font'><%= country.title %></td>
  <td><span title="<%= country.comment %>" class='truncate-text'><%= country.comment %></span></td>
  <td class="p-3">
		<div class="icon-container-1">
      <div class="my-icon-1 my-view-1">
        <div class="my-tooltip-1">
          <div class="icon-container">
            <div class="my-icon my-view">
              <div class="my-tooltip">
                Edit
              </div>
              <%if (check_can_update(@module_permission))%>
                <%= link_to edit_country_path(country), remote: true, class:"icon-div" do %>
                  <i class="fa fa-edit mt-2" aria-hidden="true"></i>
                <%end %>
              <% end %>
            </div>
            <div class="my-icon my-view">
              <div class="my-tooltip">
                Delete
              </div>
              <%if (check_can_delete(@module_permission))%>
                <%= link_to country, method: :delete, data: {title: 'Confirmation' ,confirm: 'Are you sure?', commit: 'Sure!'}, :remote => true, :class => 'delete icon-div' do %>
                  <i class="fa fa-trash mt-2" aria-hidden="true"></i>
                <%end %>
              <% end %> 
            </div>
            <div class="my-icon my-view">
              <div class="my-tooltip">
                Show
              </div>
              <%if (check_can_read(@module_permission))%>
                <%= link_to country_path(country), remote: true, class:"icon-div" do %>
                  <i class="fa fa-eye mt-2" aria-hidden="true"></i>
                <%end %>
              <%end%>    
            </div>
            <div class="my-icon my-view">
              <div class="my-tooltip">
                PDF
              </div>
              <%if (check_can_download_pdf(@module_permission))%>
               <%= link_to countries_path('q[id_eq]':country.id,'pdf':true), title: 'Download PDF', target: '_blank', class:"icon-div" do %>
                  <i class="fa fa-file-pdf text-danger mt-2 " aria-hidden="true"></i>
                <%end %>  
              <% end %>    
            </div>
            <div class="my-icon my-view">
              <div class="my-tooltip">
                CSV
              </div>
              <%if (check_can_download_csv(@module_permission))%>
                <%= link_to countries_path('q[id_eq]':country.id,'csv':true), title: 'Download CSV', class:"icon-div" do%>
                  <i class="fa fa-file-excel mt-2 text-success" aria-hidden="true"></i>
                <%end %>
              <% end %>  
            </div>
          </div>
        </div>
        <span class="icon-div-1">
              <i class="fa fa-ellipsis-v mt-2" aria-hidden="true"></i>
        </span>
      </div>
    </div>
  </td>
</tr>
