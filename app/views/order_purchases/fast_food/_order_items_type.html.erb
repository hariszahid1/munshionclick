<div id="item_type" class="row item_type">
  <div class="col-lg-12 col-sm-12 urdu-font">
    <%@item_types.each do |item_type|%>
      <div class="float-left" >
        <%= link_to item_type_products_path(item_type), class: "alert alert-info urdu-font btn btn-info font-weight-bold mr-2", remote: true do %>
          <i class="fa fa-shopping-cart" aria-hidden="true"></i>
          <%=item_type.title%>
        <% end %>
      </div>
    <% end %>
  </div>
</div>
