<tr>
  <td><%= expense_entry.expense_type.title %></td>
  <td><%= expense_entry.account.title %> ------ <%=number_with_delimiter(@expense_payment[expense_entry.id])%></td>
  <td><%= number_with_delimiter(expense_entry.amount)%></td>
  <td><%= expense_entry.comment %></td>
  <td>
    <%if expense_entry.created_at.strftime("%d%b%y")!=expense_entry.updated_at.strftime("%d%b%y")%>
      <%= expense_entry.created_at.present? ? expense_entry.created_at.strftime("%d%b%y , %I:%M") : '' %> | <%= expense_entry.updated_at.present? ? expense_entry.updated_at.strftime("%d%b%y , %I:%M") : '' %>
    <%else%>
      <%= expense_entry.created_at.present? ? expense_entry.created_at.strftime("%d%b%y , %I:%M") : '' %>
    <%end%>
  </td>
  <td>
  <div class="icon-container-1">
      <div class="my-icon-1 my-view-1">
        <div class="my-tooltip-1">
          <div class="icon-container">
            <div class="my-icon my-view">
              <div class="my-tooltip">
                Edit
              </div>
              <%if (check_can_update(@module_permission))%>
                <%= link_to edit_expense_entry_path(expense_entry), remote: true, class:"icon-div" do %>
                  <i class="fa fa-edit mt-2" aria-hidden="true"></i>
                <%end %>
              <% end %>
            </div>
            <div class="my-icon my-view">
              <div class="my-tooltip">
                Delete
              </div>
              <%if (check_can_delete(@module_permission))%>
                <%= link_to expense_entry, method: :delete, data: {title: 'Confirmation' ,confirm: 'Are you sure?', commit: 'Sure!'}, :remote => true, :class => 'delete icon-div' do %>
                  <i class="fa fa-trash mt-2" aria-hidden="true"></i>
                <%end %>
              <% end %> 
            </div>
            <div class="my-icon my-view">
              <div class="my-tooltip">
                Show
              </div>
              <%if (check_can_read(@module_permission))%>
                <%= link_to expense_entry_path(expense_entry), remote: true, class:"icon-div" do %>
                  <i class="fa fa-eye mt-2" aria-hidden="true"></i>
                <%end %>
              <%end%>    
            </div>
          </div>
        </div>
        <span class="icon-div-1">
              <i class="fa fa-ellipsis-v mt-2" aria-hidden="true"></i>
        </span>
      </div>
    </div>
  </td>
</tr>
