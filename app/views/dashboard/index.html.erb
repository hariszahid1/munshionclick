<%if pos_setting_sys_type=="batamega"%>
  <%= render 'dashboard/batamega/index_bata', products: @products %>
<%elsif pos_setting_sys_type=="Draw"%>
  <%= render 'dashboard/draw/index_draw', products: @products %>
<%elsif pos_setting_sys_type=="FastFood"%>
  <%= render 'dashboard/fast_food/index_fast_food', products: @products %>
<%elsif pos_setting_sys_type=="batha"%>
  <%= render 'dashboard/batha_production/index_batha', products: @products %>
<%elsif pos_setting_sys_type=="CustomClear"%>
  <%= render 'dashboard/custom_clear/index_custom', products: @products %>
<%elsif pos_setting_sys_type=="HousingScheme"%>
  <%= render 'dashboard/housing_scheme/index_housing', products: @products %>
<%elsif current_user&.developer? %>
  <%= render 'dashboard/developer/index_developer', products: @products %>
<%elsif pos_setting_sys_type=="Dealer"%>
  <%= render 'dashboard/dealer/index_dealer', products: @products %>
<%elsif pos_setting_sys_type=="ColdStorage"%>
  <%= render 'dashboard/cold_storage/index', products: @products %>
<%else%>
  <%= render 'dashboard/pos/index', products: @products %>
<%end%>
