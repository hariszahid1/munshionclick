      </div>

      <script type="text/javascript">
        google.charts.load("current", {packages:["corechart"]});
        google.charts.setOnLoadCallback(drawChart);
        function drawChart() {
          var data = google.visualization.arrayToDataTable([
          ["Element", "Quantity", { role: "style" } ],
          ["pathair kachi", <%=@daily_book_salary_details_raw_quantity%>, "#3ac6d6"],
          ["pathair paki", <%=@daily_book_salary_details_quantity%>, "#3ac6d6"],
          ["Khakar", <%=@daily_book_salary_details_wast %>, "silver"],
          ["Bharai", <%=@production_blocks_bricks_quantity.to_i+@production_blocks_tiles_quantity.to_i%>, "gold"],
          ["Jalai", <%=@total_item_quantity.to_i%>, "red"],
          ["Nakasi", <%=@n_total_bricks.to_i%>, "color: #d6783a"]
          ]);

          var view = new google.visualization.DataView(data);
          view.setColumns([0, 1,
          { calc: "stringify",
          sourceColumn: 1,
          type: "string",
          role: "annotation" },
          2]);

          var options = {
            title: "Production Weekly Report",
            width: 1200,
            height: 400,
            bar: {groupWidth: "100%"},
            legend: { position: "none" },
          };
          var chart = new google.visualization.BarChart(document.getElementById("barchart_values"));
          chart.draw(view, options);
        }
      </script>
      <div id="barchart_values" style="width: 1000px; height: 500px;"></div>
    </div>
 </div>
</div>

