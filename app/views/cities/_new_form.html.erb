<div class="modal fade" id="showdData" tabindex="-1" role="dialog" aria-labelledby="showdDataLabel">
  <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h3 class="modal-title ml-4" id="myModalLabel">New City</h3>
              <button type="button" class="btn btn-danger float-right" data-dismiss="modal" aria-label="Close">
          <span class="text-light">Close</span>
        </button>
      </div>
        <div class="modal-body">
          <div class="card-body">
            <%= render 'form', city: @city %>
          </div>
        </div>
          
        </div>
    </div>
</div>