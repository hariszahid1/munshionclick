<div class="col-lg-12 row cms-data-fields-container">
  <div class="col-lg-6 form-group">
    <%= cd.select :project_name, options_for_select(@project_name), {:include_blank => "Select Project Name"}, class: "form-control chosen-select disable-sys-users" %>
  </div>

  <div class="col-lg-6 form-group">
    <%= cd.select :category, options_for_select(@category), {:include_blank => "Select Category", label: 'Plot Type'}, class: "form-control chosen-select disable-sys-users" %>
  </div>
</div>
