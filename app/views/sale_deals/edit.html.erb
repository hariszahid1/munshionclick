
<div class="card shadow border-radius-1 m-2">
  <div class="card-header">
    <h3><%= params["type"] == 'ReSale' ? 'Edit ReSale Deals' : ' Edit Fresh Sale Deals' %></h3>
  </div>
  <div class="card-body">
    <%= render params['type'] == 'ReSale' ? 'form' : 'form_new', sale_deal: @sale_deal, model: @sale_deal, local: true,  method: :put, url: sale_deal_path(@sale_deal) %>
  </div>
</div>
