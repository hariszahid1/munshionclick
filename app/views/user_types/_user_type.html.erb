<tr>
  <td><%= check_box_tag 'object_ids[]', user_type.id %></td>
  <td><%= user_type.id %></td>
  <td class='urdu-font'><%= user_type.title %></td>
  <td><%= user_type.code %></td>
  <td><%= user_type.discount_by_percentage %></td>
  <td><%= user_type.discount_by_amount %></td>
  <td><span title="<%= user_type.comment %>" class='truncate-text'><%= user_type.comment %></span></td>
  <td class="p-3">
    <div class="icon-container-1">
    <div class="my-icon-1 my-view-1">
      <div class="my-tooltip-1">
    <div class="icon-container">
			<%if (check_can_update("user_types"))%>

      <div class="my-icon my-view">
        <div class="my-tooltip">
          Edit
        </div>
          <%= link_to edit_user_type_path(user_type), remote: true, class:"icon-div" do %>
            <i class="fa fa-edit mt-2" aria-hidden="true"></i>
          <% end %>
				</div>
        <% end %>

				<%if (check_can_delete("user_types"))%>

      <div class="my-icon my-view">
        <div class="my-tooltip">
          Delete
        </div>
            <%= link_to user_type, method: :delete, data: {title: 'Confirmation' ,confirm: 'Are you sure?', commit: 'Sure!'}, :remote => true, :class => 'delete icon-div' do %>
              <i class="fa fa-trash mt-2" aria-hidden="true"></i>
            <% end %>
					</div>
          <% end %>

					<%if (check_can_read("user_types"))%>

      <div class="my-icon my-view">
        <div class="my-tooltip">
          Show
        </div>
            <%= link_to user_type_path(user_type), remote: true, class:"icon-div" do %>
              <i class="fa fa-eye mt-2" aria-hidden="true"></i>
            <% end %>
					</div>
          <% end %>
					<%if (check_can_download_pdf("user_types"))%>

      <div class="my-icon my-view">
            <div class="my-tooltip">
              PDF
            </div>
                <%= link_to user_types_path('q[id_eq]':user_type.id,'pdf':true), title: 'Download PDF', target: '_blank', class:"icon-div" do %>
    			      <i class="fa fa-file-pdf text-danger mt-2 " aria-hidden="true"></i>
    		      <%end %>
      </div>
			<%end %>

			<%if (check_can_download_csv("user_types"))%>

       <div class="my-icon my-view">
            <div class="my-tooltip">
              CSV
            </div>
                  <%= link_to user_types_path('q[id_eq]':user_type.id,'csv':true), title: 'Download CSV', class:"icon-div" do%>
    			        <i class="fa fa-file-excel mt-2 text-success" aria-hidden="true"></i>
    		      <%end %>
          </div>
					<%end%>
          </div>
              </div>
              <span class="icon-div-1">
                    <i class="fa fa-ellipsis-v mt-2" aria-hidden="true"></i>
              </span>
          </div>
    </div>
  </td>
</tr>
