  <div class="card shadow-sm border-radius-1 p-2 mr-4 payment-book-index-card">
    <%= render 'map_columns/map_column_partials' %>

    <table data-toggle="table" data-search="true" class=" overflow-hidden border-radius-1">
    <thead>
      <tr>
        <th><input type="checkbox" id="master" class='select-all-checkbox' style="cursor: pointer;">
					<%if (check_can_delete(@module_permission))%>
						<div class="icon-container float-center">
							<span class="ml-2 mt-2">
