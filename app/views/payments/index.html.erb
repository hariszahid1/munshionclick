<%= search_form_for @q, url: payments_path, class: "admin-controls", method: "get" do |f| %>
    
  <div class="tab-content">
    <div class="tab-pane active mb-3 card shadow-sm mt-2" style="border-radius: 20px" id="search" role="tabpanel">
     <p>
      <div class="col-lg-10 form-group d-flex">
      <div class="col-lg-2 form-group float-left">
          <%= f.select :account_id_in, options_from_collection_for_select(@accounts, "id", ->(st){"#{st.title} \u{27F6} #{st.bank_name}"},f.object.account_id_in),{:include_blank => "Select the Account Title"},{:multiple => true,:class=>"form-control chosen-select", :data => {:placeholder => "Select the Account Title"} }%>
        </div>
        <div class="col-lg-2 form-group float-left">
          <%= f.search_field :id_eq, class: 'form-control input-sm ', placeholder: "Enter Id" %>
        </div>
        <div class="col-lg-2 form-group float-left">
          <%= f.search_field :created_at_gteq,value: @start_date, class: 'form-control input-sm datepicker', 'datepicker' => true,placeholder: "YYYY-MM-DD" %>
        </div>
        <div class="col-lg-2 form-group float-left">
          <%= f.search_field :created_at_lteq,value: @end_date, class: 'form-control input-sm datepicker', 'datepicker' => true ,placeholder: "YYYY-MM-DD"%>
        </div>
        <div class="col-lg-2 form-group float-left">
          <button class="form-control btn btn-primary" name="search_submit" type="submit" value="Search" style="width:100px;"><i class="fa fa-search"></i></button>
        </div>
        </div>
      </p>
    </div>
    <div class="tab-pane card mb-3 shadow-sm mt-2 border-radius-1" id="jama" role="tabpanel">
        <p>
        <% if can?(:delete, :all) %>
          <%= link_to new_payment_path, class: "btn btn-success ml-3 mt-3" do %>
         <i class="fa fa-plus" aria-hidden="true"></i>
          Jama/Credit
        <% end %>
        <% end %>
        </p>
    </div>
    <div class="tab-pane card mb-3 shadow-sm mt-2 border-radius-1" id="benam" role="tabpanel">
        <p>
        <% if can?(:delete, :all) %>
         <%= link_to new_payment_path(jama: true), class: "btn btn-success ml-3 mt-3" do %>
         <i class="fa fa-plus" aria-hidden="true"></i>
          Benam/Debit
        <% end %>
        <% end %>
        </p>
    </div>
    <div class="tab-pane card mb-3 shadow-sm mt-2 border-radius-1" id="transfer" role="tabpanel">
        <p>
         <%= link_to transfer_payments_path, class: "btn btn-success ml-3 mt-3" do %>
        <i class="fa fa-credit-card" aria-hidden="true"></i>
        Payment Transfer
      <% end %>
        </p>
    </div>
     <div class="tab-pane card mb-3 shadow-sm mt-2 border-radius-1" id="account" role="tabpanel">
        <p>
         <%= link_to transfer_payments_path, class: "btn btn-success ml-3 mt-3" do %>
        <i class="fa fa-credit-card" aria-hidden="true"></i>
        Account
      <% end %>
        </p>
    </div>
    <div class="tab-pane card mb-3 shadow-sm mt-2 border-radius-1" id="pdf" role="tabpanel">
      <p>
         
          <button class="btn btn-danger ml-3 mt-3" title="Download payment pdf for all user in descending order " name="submit_pdf" type="submit" value="Payment History PDF A4" formtarget="_blank">Desc <i class="fa fa-file-pdf"></i></button>
          <button class="btn btn-danger ml-3 mt-3" title="Download payment pdf for all user in ascending order" name="submit_pdf_asc" type="submit" value="Payment History PDF A4" formtarget="_blank">Asc <i class="fa fa-file-pdf"></i></button>
          <button class="btn btn-danger ml-3 mt-3" title= "Download payment pdf for single user in descending order" name="1_submit_pdf" type="submit" value="Payment History PDF A4" formtarget="_blank">1-Desc <i class="fa fa-file-pdf"></i></button>
          <button class="btn btn-danger ml-3 mt-3" title= "Download payment pdf for single user in ascending order" name="2_submit_pdf_asc" type="submit" value="Payment History PDF A4" formtarget="_blank">1-Asc <i class="fa fa-file-pdf"></i></button>
  
      </p>
    </div>
    <div class="tab-pane card mb-3 shadow-sm mt-2 border-radius-1" id="csv" role="tabpanel">
      <p>
        <div class="col-lg-2 d-flex">
        <button class="btn btn-success" title= "download payments csv" name="csv" type="submit" value="csv">Payments &nbsp;<i class="fa fa-file-excel"></i></button>
        </div>
      </p>
    </div>
    <div class="tab-pane card mb-3 shadow-sm mt-2 border-radius-1" id="emails" role="tabpanel">
      <p>
        
        <button class="btn btn-info ml-3 mt-3" type="button" data-toggle="modal" data-target="#exampleModalCenter" style="width:100px"><i class="fa fa-envelope mr-2"></i>&nbsp;Mail</button>
        <button class="btn btn-info ml-3 mt-3" name="asc_email" type="submit" value="Email"><i class="fas fa-envelope"></i> Asc email</button>
        <button class="btn btn-info ml-3 mt-3" name="desc_email" type="submit" value="Email"><i class="fas fa-envelope"></i> Desc email</button>

     </p>
      <!-- Modal -->
      <div class="modal fade" id="exampleModalCenter" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="exampleModalLongTitle">Enter your email to recieve files.</h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
              <span aria-hidden="true">&times;</span>
            </button>
            </div>
            <div class="modal-body">
              <% @pos_setting = PosSetting.first %>
              <label>Email:</label>
              <%= text_field_tag 'email_value',{}, value: current_user.email, class: 'form-control email-fields', type: 'email'%>
              <br />
              <label>Subject:</label>
              <%= text_field_tag 'subject', "#{@pos_setting.display_name} - Payment Detail report from MunshiOnClick", class: 'form-control email-fields'%>
              <br />
              <label>Body:</label><br />
              <%= text_area_tag 'body', nil, class: 'form-control'%>
              <br />

              <label>Please select one of them.</label>
              <div>
                <label style="cursor: pointer"><%= radio_button_tag :email_choice, 'CSV', required: 'required' %> CSV</label><br />
                <label style="cursor: pointer"><%= radio_button_tag :email_choice, 'PDF', required: 'required' %> PDF</label><br />
                <label style="cursor: pointer"><%= radio_button_tag :email_choice, 'Both', required: 'required' %> Both(CSV + PDF)</label><br />
              </div>
            </div>
            <div class="modal-footer">
              <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
              <button name="email" type="submit" value="email" class="btn btn-primary email-send-btn">Send</button>
            </div>
            </div>
        </div>
      </div>
    </div>
    <div class="tab-pane card mb-3 shadow-sm mt-2 border-radius-1" id="import-export" role="tabpanel">
      <p>
        <div class="d-flex">
        <button class="btn btn-success ml-2" name="export_data" type="submit" value="export_data">Export Data</button>
        <button class="btn btn-primary ml-2" type="button" data-toggle="modal" data-target="#importFile">Import Data</button>
        </div>
      </p>
    </div>
    </div>
  <% end %>
  </div>
