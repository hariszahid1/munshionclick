        <th><%= sort_link(@q, :credit, 'Credit/Jama') %></th>
        <th><%= sort_link(@q, :amount, 'Balance') %></th>
        <th>Jama/benam</th>
        <th><%= sort_link(@q, :paymentable, 'Paymentable') %></th>
