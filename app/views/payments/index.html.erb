          <td><%= number_with_delimiter(payment.debit) %></td>
          <td><%= number_with_delimiter(payment.credit) %></td>
          <td><%= number_with_delimiter(payment.amount) %></td>
          <td>
                <%if payment.amount.to_f < 0  %>
                     <b style="font-size:20px;color:blue;">Jama/Payable</b>
                <%elsif  payment.amount.to_f > 0  %>
                <b style="font-size:20px;color:red;">Benam/Receivable</b>
